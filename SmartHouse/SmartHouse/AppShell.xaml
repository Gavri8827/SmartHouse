<?xml version="1.0" encoding="UTF-8" ?>
<Shell
  xmlns="http://xamarin.com/schemas/2014/forms"
  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  xmlns:views="clr-namespace:SmartHouse"
  xmlns:utils="clr-namespace:SmartHouse.UtilitiesClass"
  xmlns:fc="clr-namespace:SmartHouse.FunClass"
  xmlns:sc="clr-namespace:SmartHouse.ShopClass"
  xmlns:hc="clr-namespace:SmartHouse.HouseCare"
  xmlns:fm="clr-namespace:SmartHouse.FamilyClass"
  xmlns:user="clr-namespace:SmartHouse.user"
  x:Class="SmartHouse.AppShell"
  BackgroundColor="#FF6A00"
  TitleColor="White"
  ForegroundColor="White">


    <ShellItem Route="login" Shell.FlyoutBehavior="Disabled">
        <ShellContent ContentTemplate="{DataTemplate user:Connect}" />
    </ShellItem>





</Shell>
