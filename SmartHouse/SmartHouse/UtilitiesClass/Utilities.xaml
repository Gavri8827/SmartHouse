<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             xmlns:local="clr-namespace:SmartHouse.UtilitiesClass" 
             x:DataType="local:UtilitiesViewModel"
             x:Class="SmartHouse.UtilitiesClass.Utilities"
            >

    <FlyoutPage.Flyout>
        <local:FlyOutMenuPage x:Name="flyout"/>
    </FlyoutPage.Flyout>

       

       
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>

                <local:UtilitiesMainPage/>
            </x:Arguments>

        </NavigationPage>

    </FlyoutPage.Detail>

</FlyoutPage>